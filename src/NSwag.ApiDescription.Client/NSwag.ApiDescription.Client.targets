      </CurrentOpenApiReference>
      <CurrentOpenApiReference>
        <Command Condition="'%(NSwagHandleReferences)' != ''">%(Command) /handleReferences:%(NSwagHandleReferences)</Command>
      </CurrentOpenApiReference>
