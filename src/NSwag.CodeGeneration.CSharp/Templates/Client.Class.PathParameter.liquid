{% if parameter.IsDateTimeArray -%}
{
    bool isAfterFirst = false;
    foreach (var item in {{ parameter.VariableName }})
    {
        if (isAfterFirst) urlBuilder_.Append(',');
        urlBuilder_.Append({{ GlobalSystemNamespaceAlias }}.Uri.EscapeDataString(item.ToString("{{ ParameterDateTimeFormat }}", {{ GlobalSystemNamespaceAlias }}.Globalization.CultureInfo.InvariantCulture)));
        isAfterFirst = true;
    }
}
{% elsif parameter.IsDateArray -%}
{
    bool isAfterFirst = false;
    foreach (var item in {{ parameter.VariableName }})
    {
        if (isAfterFirst) urlBuilder_.Append(',');
        urlBuilder_.Append({{ GlobalSystemNamespaceAlias }}.Uri.EscapeDataString(item.ToString("{{ ParameterDateFormat }}", {{ GlobalSystemNamespaceAlias }}.Globalization.CultureInfo.InvariantCulture)));
        isAfterFirst = true;
    }
}
{% elsif parameter.IsDateTime -%}
urlBuilder_.Append({{ GlobalSystemNamespaceAlias }}.Uri.EscapeDataString({{ parameter.VariableName }}.ToString("{{ ParameterDateTimeFormat }}", {{ GlobalSystemNamespaceAlias }}.Globalization.CultureInfo.InvariantCulture)));
{% elsif parameter.IsDate -%}
urlBuilder_.Append({{ GlobalSystemNamespaceAlias }}.Uri.EscapeDataString({{ parameter.VariableName }}.ToString("{{ ParameterDateFormat }}", {{ GlobalSystemNamespaceAlias }}.Globalization.CultureInfo.InvariantCulture)));
{% elsif parameter.IsArray -%}
{
    bool isAfterFirst = false;
    foreach (var item in {{ parameter.VariableName }})
    {
        if (isAfterFirst) urlBuilder_.Append(',');
        urlBuilder_.Append(ConvertToString(item, {{ GlobalSystemNamespaceAlias }}.Globalization.CultureInfo.InvariantCulture));
        isAfterFirst = true;
    }
}
{% else -%}
urlBuilder_.Append({{ GlobalSystemNamespaceAlias }}.Uri.EscapeDataString(ConvertToString({{ parameter.VariableName }}, {{ GlobalSystemNamespaceAlias }}.Globalization.CultureInfo.InvariantCulture)));
{%- endif %}